{
  "name": "@douganderson444/sveltana",
  "version": "0.0.4",
  "description": "Solana packaged into a Svelte Component for use in Svelte, Browsers, and REPLs",
  "files": [
    "dist"
  ],
  "main": "./dist/sveltana.umd.js",
  "module": "./dist/sveltana.es.js",
  "exports": {
    ".": {
      "import": "./dist/sveltana.es.js",
      "require": "./dist/sveltana.umd.js"
    }
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package:up": "svelte-kit package",
    "publish:npm": "npm publish ./package --access public",
    "build:rollup": "rollup -c"
  },
  "dependencies": {
    "@solana/spl-token": "^0.1.4",
    "@solana/spl-token-registry": "^0.2.114",
    "@solana/web3.js": "^1.12.0",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "next",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.34.0"
  },
  "type": "module",
  "keywords": [
    "solana",
    "svelte",
    "repl",
    "browser",
    "douganderson444"
  ],
  "homepage": "https://github.com/DougAnderson444/sveltana",
  "bugs": "https://github.com/DougAnderson444/sveltana",
  "license": "MIT",
  "author": "Doug Anderson <douganderson444@gmail.com>",
  "funding": "Doug Anderson <douganderson444@gmail.com>",
  "repository": "https://github.com/DougAnderson444/sveltana"
}
